	<queryString>
		<![CDATA[select iv.prod_codigo,p.prod_descricao, p.prod_qtdeEmbalagem,um.um_sigla, iv.itv_qtde, p.prod_preco, (iv.itv_qtde*p.prod_preco) as total,
v.ven_total, v.ven_data, f.cli_nome as funcionario, v.ven_cliNome as cliente
from venda v
inner join itens_venda iv on v.ven_codigo = iv.ven_codigo
inner join produto p on p.prod_codigo = iv.prod_codigo
inner join cliente f on f.cli_codigo = v.funcionario_cli_codigo
inner join unidade_medida um on um.um_codigo = p.um_codigo]]>
	</queryString>
