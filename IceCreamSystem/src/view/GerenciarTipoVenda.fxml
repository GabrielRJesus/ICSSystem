<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="585.0" styleClass="background" stylesheets="@estilo.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.GerenciarTipoVendaController">
   <children>
      <Label layoutX="76.0" layoutY="29.0" text="Gerenciar Tipos de Venda" textFill="#2b2553">
         <font>
            <Font name="Arial Black" size="30.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtCodigo" focusColor="#2b2553" layoutX="40.0" layoutY="104.0" prefHeight="25.0" prefWidth="90.0" promptText="Código:" styleClass="corTexto" stylesheets="@estilo.css">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtDescricao" focusColor="#2b2553" layoutX="148.0" layoutY="104.0" prefHeight="25.0" prefWidth="390.0" promptText="Descrição:" styleClass="corTexto" stylesheets="@estilo.css">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXTextField>
      <TableView fx:id="tabelaTipoVenda" layoutX="40.0" layoutY="158.0" onMouseClicked="#selecionarTipoVenda" prefHeight="345.0" prefWidth="497.0">
        <columns>
          <TableColumn fx:id="colCodigo" prefWidth="98.0" text="Código" />
          <TableColumn fx:id="colTipoVenda" prefWidth="485.0" text="Tipo de Venda" />
        </columns>
      </TableView>
      <JFXButton fx:id="btNovo" layoutX="48.0" layoutY="524.0" onAction="#clkNovo" prefHeight="31.0" prefWidth="90.0" stylesheets="@estilo.css" text="Novo" textFill="#2b2553">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btGravar" layoutX="148.0" layoutY="524.0" onAction="#clkGravar" prefHeight="31.0" prefWidth="90.0" stylesheets="@estilo.css" text="Gravar" textFill="#2b2553">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btCancelar" layoutX="245.0" layoutY="524.0" onAction="#clkCancelar" prefHeight="31.0" prefWidth="90.0" stylesheets="@estilo.css" text="Cancelar" textFill="#2b2553">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btExcluir" layoutX="342.0" layoutY="524.0" onAction="#clkExcluir" prefHeight="31.0" prefWidth="90.0" stylesheets="@estilo.css" text="Excluir" textFill="#2b2553">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btSair" layoutX="439.0" layoutY="524.0" onAction="#clkSair" prefHeight="31.0" prefWidth="90.0" stylesheets="@estilo.css" text="Sair" textFill="#2b2553">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
